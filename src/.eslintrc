{
	"parser": "babel-eslint",
	"env": {
		"browser": true
	},
	"plugins": ["react"],
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"rules": {
		"no-console": 1,
		"no-inline-comments": "warn",
		"no-undefined": "error",
		"no-undef": "error",
		"no-mixed-spaces-and-tabs": "off",
		"no-throw-literal": "off"
	},
	"globals": {
		"__DEV__": true,
		"__dirname": false,
		"__fbBatchedBridgeConfig": false,
		"alert": false,
		"cancelAnimationFrame": false,
		"cancelIdleCallback": false,
		"clearImmediate": true,
		"clearInterval": false,
		"clearTimeout": false,
		"console": true,
		"document": false,
		"describe": true,
		"it": true,
		"expect": true,
		"escape": false,
		"Event": false,
		"EventTarget": false,
		"exports": false,
		"fetch": false,
		"FormData": false,
		"global": false,
		"location": true,
		"jest": false,
		"Map": true,
		"module": false,
		"navigator": false,
		"process": false,
		"Promise": true,
		"requestAnimationFrame": true,
		"requestIdleCallback": true,
		"require": false,
		"Set": true,
		"setImmediate": true,
		"setInterval": false,
		"setTimeout": false,
		"window": false,
		"XMLHttpRequest": false,
		"pit": false,
		"jasmine": true
	}
}
